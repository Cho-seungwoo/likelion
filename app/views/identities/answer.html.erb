<h1>나의 습관 </h1>

<% current_user.answers.each do |answer| %>
    <%= answer.question.body %>
    <hr>
<% end if current_user.answers.present? %>


<% if @selected_a != nil %>

<form method="GET" action='/identities/feedback'>
    <% Question.where(identity_id: @selected_a).each do |question| %> 
	<input type="checkbox" name="selector_b" value="<%=question.id%>" /><%=@selected_a%> <br> <!-- param[:selector] = A -->
<% end %> 
<input type="submit" value="Send to receiver" />

</form>
<% end %> 
