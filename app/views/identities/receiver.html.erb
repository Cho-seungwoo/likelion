<h1>습관 선택 </h1>

<% if @selected != nil %>

<%= @selected %>

<form action="/identities/answer" method="POST">
   <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <% Question.where(identity_id: @selected).each do |question| %> 
	    <input type="checkbox" name="selector[]" value="<%=question.id%>" /><%=question.body%> <br> <!-- param[:selector] = A -->
    <% end %> 

<input type="submit" value="Send to receiver" />

</form>
<% end %> 


   